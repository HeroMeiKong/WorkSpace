var nameSub = function (name) {
    if (name.length > 12) {
        return name.substring(0, 12) + '...'
    } else {
        return name
    }
}
var numModel = function (num) {
    var num = parseInt(num)
    if (num < 10000) {
        return num
    }
    // else if (num >= 1000 && num < 10000) {
    //     var t = num / 1000
    //     return t.toFixed(1) + 'k'
    // }
    else if (num >= 10000) {
        var t = num / 10000
        return t.toFixed(1) + 'ä¸‡'
    }
}

module.exports = {
    nameSub: nameSub,
    numModel: numModel,
};