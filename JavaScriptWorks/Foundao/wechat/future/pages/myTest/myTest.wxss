.dianwowrapper{
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: center;
}

.dianwo{
  position: absolute;
  bottom: 0;
  font-size: 40px;
}

.publish {
  position: absolute;
  width: 100%;
  height: 100%;
  /* background-color: rgba(46,46,46,.8); */
  background-color: #931E23;/* 春节样式 */
  /* background-color: rgba(0, 0, 0, 1); */
  display: flex;
  /* justify-content: center; */
  align-items: center;
  flex-direction: column;
  /* padding: 10rpx 0; */
  top: 0;
  left: 0;
}

#myVideo1 {
  border-radius: 14rpx;
  position: relative;
}

#myVideo1 .playimg {
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  position: absolute;
  border-radius: 14rpx;
}

#myVideo1 .playimg cover-image {
  width: 120rpx;
  height: 120rpx;
}

.overcover,
.submissionwrapper{
  width: 100%;
  height: 100%;
  z-index: 10;
  position: absolute;
  background-color: rgba(46,46,46,.8);
  top: 0;
  left: 0;
}