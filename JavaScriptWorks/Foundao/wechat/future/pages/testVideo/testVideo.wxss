.wrapper{
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}

.innervideo{
  width: 100%;
  height: 100%;
}

.cover{
  width: 100%;
  height: 100%;
  position: absolute;
}

.topbutton{
  width: 100%;
}

.backbutton{
  width: 90rpx;
  height: 90rpx;
  padding: 20rpx;
}

.back-btn{
  position: absolute;
  top:0;
  left:0;
  width: 84rpx;
  height: 142rpx;
}

.back-btn-img{
  position: absolute;
  top:50%;
  left:50%;
  transform: translate(-50%,-50%);
  width: 24rpx;
  height: 42rpx;
}

.iphoneX-back-btn{
  height:245rpx;
}